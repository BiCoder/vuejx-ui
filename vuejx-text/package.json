  "main": "dist/vuejx-text.min.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/*.vue",
    "build": "npm run build:cjs",
    "build:cjs": "poi build --format cjs --dist ../../vuejx-client/src/plugins/ && rm -rf ../../vuejx-client/src/plugins/vuejx-text.min.js.map",
    "build:umd": "poi build --format umd --dist",
    "build:doc": "vue-styleguidist build",
    "serve": "vue-styleguidist server",
    "test": "poi test",
    "test:watch": "poi test --watch",
    "test:cov": "poi test --coverage",
    "dev": "vue-cli-service serve"
